{
  "compilerOptions": {
    "target": "ES2021",
    "esModuleInterop": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "outDir": "./.medusa/server",
    "rootDir": "./",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "checkJs": false,
    "strictNullChecks": true,
    "paths": {
      "#/*": ["src/*"],
      "@mercurjs/framework": ["../../packages/framework/src"],
      "@mercurjs/framework/*": ["../../packages/framework/src/*"],
      "@mercurjs/configuration": ["../../packages/modules/configuration/src"],
      "@mercurjs/configuration/*": ["../../packages/modules/configuration/src/*"],
      "@mercurjs/marketplace": ["../../packages/modules/marketplace/src"],
      "@mercurjs/marketplace/*": ["../../packages/modules/marketplace/src/*"],
      "@mercurjs/seller": ["../../packages/modules/seller/src"],
      "@mercurjs/seller/*": ["../../packages/modules/seller/src/*"],
      "@mercurjs/reviews": ["../../packages/modules/reviews/src"],
      "@mercurjs/reviews/*": ["../../packages/modules/reviews/src/*"],
      "@mercurjs/payout": ["../../packages/modules/payout/src"],
      "@mercurjs/payout/*": ["../../packages/modules/payout/src/*"],
      "@mercurjs/brand": ["../../packages/modules/brand/src"],
      "@mercurjs/brand/*": ["../../packages/modules/brand/src/*"],
      "@mercurjs/commission": ["../../packages/modules/commission/src"],
      "@mercurjs/commission/*": ["../../packages/modules/commission/src/*"],
      "@mercurjs/wishlist": ["../../packages/modules/wishlist/src"],
      "@mercurjs/wishlist/*": ["../../packages/modules/wishlist/src/*"],
      "@mercurjs/attribute": ["../../packages/modules/attribute/src"],
      "@mercurjs/attribute/*": ["../../packages/modules/attribute/src/*"],
      "@mercurjs/order-return-request": ["../../packages/modules/order-return-request/src"],
      "@mercurjs/order-return-request/*": ["../../packages/modules/order-return-request/src/*"],
      "@mercurjs/requests": ["../../packages/modules/requests/src"],
      "@mercurjs/requests/*": ["../../packages/modules/requests/src/*"],
      "@mercurjs/split-order-payment": ["../../packages/modules/split-order-payment/src"],
      "@mercurjs/split-order-payment/*": ["../../packages/modules/split-order-payment/src/*"],
      "@mercurjs/taxcode": ["../../packages/modules/taxcode/src"],
      "@mercurjs/taxcode/*": ["../../packages/modules/taxcode/src/*"],
      "@mercurjs/algolia": ["../../packages/modules/algolia/src"],
      "@mercurjs/algolia/*": ["../../packages/modules/algolia/src/*"],
      "@mercurjs/resend": ["../../packages/modules/resend/src"],
      "@mercurjs/resend/*": ["../../packages/modules/resend/src/*"],
      "@mercurjs/stripe-tax-provider": ["../../packages/modules/stripe-tax-provider/src"],
      "@mercurjs/stripe-tax-provider/*": ["../../packages/modules/stripe-tax-provider/src/*"],
      "@mercurjs/payment-stripe-connect": ["../../packages/modules/payment-stripe-connect/src"],
      "@mercurjs/payment-stripe-connect/*": ["../../packages/modules/payment-stripe-connect/src/*"]
    }
  },
  "ts-node": {
    "swc": true
  },
  "include": ["**/*", ".medusa/types/*"],
  "exclude": ["node_modules", ".medusa/server", ".medusa/admin", ".cache"]
}
